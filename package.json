{
  "name": "sales-api",
  "version": "1.0.0",
  "description": "An API for dealing with sales",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec \"ts-node src/main/server.ts\" -e ts",
    "lint": "eslint . --fix",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --watch --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest.unit.config.js",
    "test:integration": "npm test -- --watch -c jest.integration.config.js",
    "test:staged": "npm test -- --findRelatedTests .",
    "test:ci": "npm test -- --coverage",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "compile": "npm run build && node dist/main/server.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feitosadavi/sales-api.git"
  },
  "keywords": [
    "sale",
    "sales",
    "api",
    "sales-api",
    "salesapi",
    "sales",
    "api"
  ],
  "author": "Davi Feitosa de Freita",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/feitosadavi/sales-api/issues"
  },
  "homepage": "https://github.com/feitosadavi/sales-api#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jest": "^27.5.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2"
  },
  "_moduleAliases": {
    "@": "sales-api"
  }
}
